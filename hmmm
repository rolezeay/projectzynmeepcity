local Toggle = Tab:CreateToggle({
   Name = "Loop Soltar Varas",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(value)
       isSpamming = value
        
       if isSpamming then
           spawn(function()
               while isSpamming do
                   game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.Backspace, false, game)
                   wait(0)
               end
           end)
       end
   end,
