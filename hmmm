                    local inviteFromArgs = { [1] = jogador.UserId }
                    ReplicatedStorage.FunctionConnections.RequestInviteFromFriend:InvokeServer(unpack(inviteFromArgs))

                    local sendInviteArgs = { [1] = jogador.UserId }
