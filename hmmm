local Section = Tab:CreateSection("Espalhador de BalÃµes")

local Dropdown = Tab:CreateDropdown({
   Name = "Selecionar balÃ£o",
   Options = {"Vermelho", "Azul", "Verde", "Roxo", "Branco", "CoraÃ§Ã£o"},
   CurrentOption = {"Vermelho"},
   MultipleOptions = false,
   Flag = "BalloonColor",
   Callback = function(Options)
      selectedBalloonColor = Options[1]
   end,
})

local Toggle = Tab:CreateToggle({
   Name = "Iniciar o espalhamento de balÃµes",
   CurrentValue = false,
   Flag = "SpawnBalloons",
   Callback = function(Value)
      spawnBalloons = Value
      if spawnBalloons then
         task.spawn(function()
            while spawnBalloons do
               local balloonID
               if selectedBalloonColor == "Vermelho" then
                  balloonID = 1311
               elseif selectedBalloonColor == "Azul" then
                  balloonID = 1312
               elseif selectedBalloonColor == "Verde" then
                  balloonID = 1313
               elseif selectedBalloonColor == "Roxo" then
                  balloonID = 1314
               elseif selectedBalloonColor == "Branco" then
                  balloonID = 1315
               elseif selectedBalloonColor == "CoraÃ§Ã£o" then
                  balloonID = 1039
               end

               local args = {
                  [1] = 201,
                  [2] = balloonID,
                  [3] = {}
               }

               local args2 = {
                  [1] = 202
               }

               game:GetService("ReplicatedStorage").Connection:InvokeServer(unpack(args))
               game:GetService("ReplicatedStorage").Connection:InvokeServer(unpack(args2))

               task.wait(0)
            end
         end)
      end
   end,
})

